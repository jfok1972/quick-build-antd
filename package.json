{
  "name": "quick-build-antd",
  "version": "4.0.0",
  "license": "MIT",
  "private": true,
  "description": "快速架构系统antd版本",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.5",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.13.2",
    "@ant-design/pro-layout": "^6.16.6",
    "@antv/s2": "^1.14.0",
    "@antv/s2-react": "^1.13.0",
    "@madnessxd/react-orgchart": "^1.0.9",
    "ajax-js": "^1.0.17",
    "animate.css": "^4.1.1",
    "antd": "^4.19.5",
    "bpmn-js": "^7.3.0",
    "bpmn-js-properties-panel": "^0.35.0",
    "classnames": "^2.3.1",
    "dva": "^2.6.0-beta.22",
    "echarts": "^5.2.2",
    "font-awesome": "^4.7.0",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.11",
    "marked": "^4.0.12",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "object-to-formdata": "^4.1.0",
    "omit.js": "^2.0.2",
    "path-to-regexp": "2.4.0",
    "pinyin-match": "^1.2.2",
    "qs": "^6.9.0",
    "re-resizable": "^6.5.5",
    "react": "^17.0.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.9",
    "react-highlight-words": "^0.17.0",
    "react-icons": "^4.2.0",
    "react-json-editor-ajrm": "^2.5.13",
    "react-rnd": "^10.3.3",
    "react-router": "^5.2.0",
    "react-zmage": "0.8.5-beta.36",
    "redux": "^4.1.2",
    "sm-crypto": "^0.3.7",
    "umi": "^3.5.21",
    "umi-request": "^1.4.0",
    "use-merge-value": "^1.0.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^25.1.0",
    "@types/lodash": "^4.14.144",
    "@types/marked": "^4.0.2",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-json-editor-ajrm": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@umijs/fabric": "^2.0.5",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/preset-ui": "^2.0.9",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^3.0.0",
    "stylelint": "^13.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
